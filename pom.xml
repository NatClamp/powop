	</properties>
	<ciManagement>
		<system>jenkins</system>
		<url>http://129.67.24.160/ci</url>
	</ciManagement>
	<scm>
		<connection>scm:cvs:pserver:anonymous:@129.67.24.160:/var/cvs:emonocot</connection>
		<developerConnection>scm:cvs:pserver:${developer.username}:@129.67.24.160:/var/cvs:emonocot</developerConnection>
