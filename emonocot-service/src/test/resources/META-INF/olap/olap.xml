<?xml version="1.0" encoding="UTF-8"?>
<Schema name="emonocot">
    <Cube name="Job">
        <Table name="ANNOTATION"/>
        <Dimension name="taxa" foreignKey="ANNOTATEDOBJID">
            <Hierarchy hasAll="true" primaryKey="ID">
                <Table name="TAXON"/>
                <Level name="family" column="FAMILY" uniqueMembers="true"/>
                <Level name="genus" column="GENUS" uniqueMembers="true"/>
                <Level name="species" column="SPECIFICEPITHET" uniqueMembers="true"/>
            </Hierarchy>
        </Dimension>
        <Dimension name="Job" foreignKey="JOBID">
			<Hierarchy hasAll="true" allMemberName="Globally"
				primaryKey="JOB_EXECUTION_ID">
				<Table name="BATCH_JOB_EXECUTION" />
				<Level name="Job" column="JOB_INSTANCE_ID" uniqueMembers="true" />
			</Hierarchy>
		</Dimension>
		<Dimension name="Object Type">
			<Hierarchy hasAll="true">
				<Level name="Object type" column="ANNOTATEDOBJTYPE"
					uniqueMembers="true" />
			</Hierarchy>
		</Dimension>
		<Dimension name="Code">
			<Hierarchy hasAll="true">
				<Level name="Code" column="CODE" uniqueMembers="true" />
			</Hierarchy>
		</Dimension>
		<Dimension name="Type">
			<Hierarchy hasAll="true">
				<Level name="Type" column="TYPE" uniqueMembers="true" />
			</Hierarchy>
		</Dimension>
		<Measure name="Object Numbers" column="ANNOTATEDOBJID"
			aggregator="distinct-count" formatString="#,###" />
		<Measure name="Annotation Numbers" column="ID" aggregator="distinct-count"
			formatString="#,###" />
    </Cube>
</Schema>
